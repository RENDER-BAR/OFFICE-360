		<plugin name="radar"
			url="plugins/radar.js"
			align="centr"
			edge="center"
			x="0" y="0"
			scale="0.5"
			heading="0.0"
			headingoffset="0"
			fillcolor="0x12B8EE"
			fillalpha="0.5"
			linewidth="1"
			linecolor="0x12B8EE"
			linealpha="0.3"
			invert="false"
			/>
	
		<layer name="activespot" 
			type="container"
			width="15"
			height="15"
			bgalpha="0.6"
			bgroundedge="20"
			bgborder="1 0x93FF89 1"
			bgcolor="0x098894"
			bgshadow="0 0 10 3 0x000000 0.05"
			align="center"
			edge="center"
			enabled="true"
			x="0"
			y="0"
			/>
	</layer>
	<action name="updateradar">
		trace(%1);
		set(layer[activespot].parent, %1);
		set(layer[radar].parent, %1);
		tween(layer[radar].heading, %2);
	</action>
	<action name="toggle_floor_plan_size">
	set(state, get(layer[floorplans_container].state));
		
		
		if(get(state) == minimized,
		
			<!-- if state is minimized -->
			if(device.mobile,
				tween(layer[floorplans_container].scale, 0.2);
				,
				tween(layer[floorplans_container].scale, 0.2);
			);
			
			set(layer[floorplans_container].state, maximized);
			,
			
			<!-- if state is maximized -->
			
			if(device.mobile,
				tween(layer[floorplans_container].scale, 0.5);
				,
				tween(layer[floorplans_container].scale, 0.5);
			);
			
			set(layer[floorplans_container].state, minimized);
			
		);
	</action>
	
